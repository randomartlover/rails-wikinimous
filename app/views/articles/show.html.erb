<h2><%= @article.title %></h2>
<%= link_to edit_article_path(@article) do%>
  <i class="fa fa-pencil" aria-hidden="true"></i>
<% end %>
<%= link_to article_path(@article),method: :delete, data: {confirm: "Are you sure?"}, class: "text-dark-blue" do %>
    <i class="fa fa-trash-o" aria-hidden="true"></i>
<% end %>
<hr>
<p><%= @article.content %></p>
